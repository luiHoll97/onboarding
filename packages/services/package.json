{
  "name": "@driver-onboarding/services",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "db:pg:up": "docker compose -f docker-compose.postgres.yml up -d",
    "db:pg:down": "docker compose -f docker-compose.postgres.yml down",
    "db:pg:logs": "docker compose -f docker-compose.postgres.yml logs -f postgres",
    "db:pg:migrate": "bash scripts/pg-migrate.sh"
  },
  "dependencies": {
    "@driver-onboarding/proto": "*",
    "@types/pg": "8.11.6",
    "cors": "2.8.5",
    "express": "4.21.2",
    "pg": "8.11.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "tsx": "4.19.2",
    "typescript": "~5.7.2"
  }
}
